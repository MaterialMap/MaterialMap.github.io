{
  "name": "material-map",
  "version": "1.0.0",
  "description": "LS-DYNA Material Models Database",
  "homepage": "https://MaterialMap.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaterialMap/MaterialMap.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/MaterialMap/MaterialMap.github.io/issues"
  },
  "scripts": {
    "dev": "npm run serve",
    "serve": "python3 -m http.server 5500",
    "test": "playwright test",
    "build": "npm run generate-file-list",
    "generate-file-list": "node scripts/generate-file-list.js",
    "lint": "eslint src/js/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "validate-data": "node scripts/validate-toml.js",
    "import-to-supabase": "node scripts/import-to-supabase.js",
    "import-to-supabase:dry-run": "node scripts/import-to-supabase.js --dry-run",
    "supabase:stats": "node -e \"import('./src/js/config/supabase.js').then(m => m.materialsAPI.getStats().then(console.log))\""
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "eslint": "^8.56.0",
    "playwright": "^1.54.2",
    "prettier": "^3.1.0",
    "toml": "^3.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "http-server": "^14.1.1"
  }
}
